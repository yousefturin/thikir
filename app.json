{
  "expo": {
    "name": "Al Mufrdun",
    "slug": "almufrdun",
    "sdkVersion": "51.0.38",
    "description": "Islamic application.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/Images/splashEntro.png",
      "resizeMode": "contain",
      "backgroundColor": "#151515"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Images/splashEntro.png",
        "backgroundColor": "#151515"
      },
      "permissions": ["android.permission.NOTIFICATION"]
    },
    "web": {
      "favicon": "./assets/Images/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "82006e3a-5c19-4949-9631-069673d77d93"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/82006e3a-5c19-4949-9631-069673d77d93"
    },
    "plugins": ["expo-localization"]
  }
}
