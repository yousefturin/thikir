{
  "expo": {
    "name": "Almufdun",
    "slug": "Almufdun",
    "description": "Islamic application that sorts all athkar under one app, as well as giving you random Verses from Quran, a Hadith from Sahih muslim, and a Dua",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#151515"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon.png",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-144.png",
        "backgroundColor": "#151515"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
