{
  "expo": {
    "name": "Mufradun",
    "slug": "mufradun",
    "description": "Islamic application.",
    "version": "v1.1.1-android-dev",
    "orientation": "portrait",
    "icon": "./assets/Images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/Images/splashEntro.png",
      "resizeMode": "contain",
      "backgroundColor": "#151515"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Images/icon.png",
        "backgroundColor": "#151515"
      },
      "permissions": [
        "android.permission.NOTIFICATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.yusefturin.almufrdun"
    },
    "web": {
      "favicon": "./assets/Images/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "82006e3a-5c19-4949-9631-069673d77d93"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/82006e3a-5c19-4949-9631-069673d77d93"
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
